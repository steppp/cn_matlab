x = linspace(0, 1, 10)

x =

         0    0.1111    0.2222    0.3333    0.4444    0.5556    0.6667    0.7778    0.8889    1.0000

y=x'

y =

         0
    0.1111
    0.2222
    0.3333
    0.4444
    0.5556
    0.6667
    0.7778
    0.8889
    1.0000

a = ones(2)

a =

     1     1
     1     1

b = [1 2 3, 4 5 6]

b =

     1     2     3     4     5     6

b = [1 2 3; 4 5 6]

b =

     1     2     3
     4     5     6

b = [1 2; 3 4]

b =

     1     2
     3     4

c = a+b

c =

     2     3
     4     5

d=b-a

d =

     0     1
     2     3

e = a*b

e =

     4     6
     4     6

el=a.*b

el =

     1     2
     3     4

%moltiplicaizone elemento per elemento
eldiv = a./b

eldiv =

    1.0000    0.5000
    0.3333    0.2500

% divisione elemento per elemento
a^2

ans =

     2     2
     2     2

% prodotto matrice vettore
a.^2

ans =

     1     1
     1     1

b.^2

ans =

     1     4
     9    16

% elevamento al quadrato della matrice
sin(a)

ans =

    0.8415    0.8415
    0.8415    0.8415

% calcolo del seno per ogni elemento di a

